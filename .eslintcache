li>\r\n                                        )\r\n                                    })\r\n                                }\r\n                            </ul>\r\n                        )\r\n                    }\r\n                        <form onSubmit={handleSubmit} action=\"\">\r\n                            <div className=\"email-signIn\">\r\n                                \r\n                            <FormInput  \r\n                                type=\"email\"\r\n                                name=\"email\"\r\n                                handleChange={e => setEmail(e.target.value)}\r\n                                value={email}\r\n                                placeholder=\"Email\"\r\n                                \r\n                                />\r\n                                <FormInput  \r\n                                type=\"password\"\r\n                                name=\"password\"\r\n                                handleChange={e => setPassword(e.target.value)}\r\n                                value={password}\r\n                                placeholder=\"Password\"\r\n                                \r\n                                />\r\n                                \r\n                                <Button\r\n                                    onClick={signInUser} \r\n                                    className=\"button\">\r\n                                    LOGIN\r\n                                </Button>\r\n                            </div>\r\n                                {/* sign-in with google */}\r\n                            <div className=\"social signIn\">\r\n                                <Button pay onClick={signInWithGoogle}>\r\n                                    Sign In with Google\r\n                                </Button>\r\n                            </div>\r\n                            {/* forgot password  */}\r\n                            <div className=\"links\